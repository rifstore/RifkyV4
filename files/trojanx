URL="https://api.telegram.org/bot$KEY/sendMessage"
TEXT="
<code>ğ–£âšâšâšâšâšâšâšğ–£˜âšâšâšâšâšâšâšğ–£</code>
ğ’‚Notifikasi Multilogin Trojan
<code>ğ–£âšâšâšâšâšâšâšğ–£˜âšâšâšâšâšâšâšğ–£</code>
<code>ğ’‚Username :</code> <code>$user</code>
<code>ğ’‚Limit IP :</code> <code>$iplimit</code>
<code>ğ’‚Login IP :</code> <code>$cekcek</code>
<code>ğ–£âšâšâšâšâšâšâšğ–£˜âšâšâšâšâšâšâšğ–£</code>"

curl -s --max-time $TIME -d "chat_id=$CHATID&disable_web_page_preview=1&text=$TEXT&parse_mode=html" $URL >/dev/null
}

